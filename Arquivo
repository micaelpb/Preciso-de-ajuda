function ehNumeroDriven(numero){
   let expo = [1,2,3,4,5,6,7,8,9]
   let formula = (5 ** expo) - 2
   for(let i = 0; i < expo.length; i++){
     formula += expo[i]
     
     if(formula === numero){
     return "É desse tipo"
     }else{
       return "Não é desse tipo"
     }
   }
 }
console.log(ehNumeroDriven())
